{
    "name": "jonnitto/picture",
    "type": "neos-plugin",
    "license": "MIT",
    "description": "Responsive picture for Neos CMS",
    "keywords": ["flow", "neos", "picture", "photo", "image"],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "~3.0 || ~4.0",
        "carbon/link": "^1.0",
        "carbon/image": "^1.3",
        "carbon/notification": "^1.2 || ^2.0"
    },
    "archive": {
        "exclude": [".editorconfig", ".github", ".gitattributes", "CODE_OF_CONDUCT.md"]
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\Picture\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Jonnitto.Picture",
        "neos": {
            "package-key": "Jonnitto.Picture"
        }
    }
}
